{
	"name": "automattic/jetpack",
	"description": "Jetpack supercharges your self‑hosted WordPress site with the awesome cloud power of WordPress.com",
	"homepage": "https://jetpack.com/",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"support": {
		"issues": "https://github.com/Automattic/jetpack/issues"
	},
	"require": {
		"ext-fileinfo": "*",
		"ext-json": "*",
		"ext-openssl": "*",
		"automattic/block-delimiter": "^0.3.4",
		"automattic/jetpack-a8c-mc-stats": "^3.0.5",
		"automattic/jetpack-account-protection": "^0.2.7",
		"automattic/jetpack-admin-ui": "^0.5.11",
		"automattic/jetpack-assets": "^4.3.10",
		"automattic/jetpack-autoloader": "^5.0.11",
		"automattic/jetpack-backup": "^4.2.25",
		"automattic/jetpack-blaze": "^0.26.12",
		"automattic/jetpack-blocks": "^3.1.1",
		"automattic/jetpack-boost-speed-score": "^0.4.10",
		"automattic/jetpack-classic-theme-helper": "^0.13.21",
		"automattic/jetpack-compat": "^4.0.1",
		"automattic/jetpack-composer-plugin": "^4.0.5",
		"automattic/jetpack-config": "^3.1.1",
		"automattic/jetpack-connection": "^6.18.12",
		"automattic/jetpack-constants": "^3.0.8",
		"automattic/jetpack-device-detection": "^3.0.9",
		"automattic/jetpack-error": "^3.0.6",
		"automattic/jetpack-external-connections": "^0.1.4",
		"automattic/jetpack-external-media": "^0.5.6",
		"automattic/jetpack-forms": "^6.8.0",
		"automattic/jetpack-image-cdn": "^0.7.21",
		"automattic/jetpack-import": "^0.9.11",
		"automattic/jetpack-ip": "^0.4.10",
		"automattic/jetpack-jitm": "^4.3.7",
		"automattic/jetpack-licensing": "^3.1.0",
		"automattic/jetpack-logo": "^3.0.5",
		"automattic/jetpack-masterbar": "^0.24.0",
		"automattic/jetpack-my-jetpack": "^5.27.3",
		"automattic/jetpack-paypal-payments": "^0.5.8",
		"automattic/jetpack-plugins-installer": "^0.5.7",
		"automattic/jetpack-post-list": "^0.8.31",
		"automattic/jetpack-publicize": "^0.66.16",
		"automattic/jetpack-redirect": "^3.0.9",
		"automattic/jetpack-roles": "^3.0.8",
		"automattic/jetpack-search": "^0.52.21",
		"automattic/jetpack-stats": "^0.17.2",
		"automattic/jetpack-stats-admin": "^0.29.1",
		"automattic/jetpack-status": "^6.0.4",
		"automattic/jetpack-subscribers-dashboard": "^0.4.2",
		"automattic/jetpack-sync": "^4.22.0",
		"automattic/jetpack-videopress": "^0.32.9",
		"automattic/jetpack-waf": "^0.27.4",
		"automattic/woocommerce-analytics": "^0.10.0",
		"scssphp/scssphp": "1.13.0"
	},
	"require-dev": {
		"antecedent/patchwork": "^2.2",
		"automattic/jetpack-changelogger": "^6.0.7",
		"automattic/patchwork-redefine-exit": "^2.0.8",
		"automattic/phpunit-select-config": "^1.0.3",
		"yoast/phpunit-polyfills": "^4.0.0"
	},
	"scripts": {
		"build-production": [
			"pnpm run build-production-concurrently"
		],
		"build-development": [
			"pnpm run build-concurrently"
		],
		"phpunit": [
			"tests/action-phpunit.sh"
		],
		"test-coverage": [
			"@php tools/build-module-headings-translations.php",
			"tests/action-test-coverage.sh"
		],
		"test-js": [
			"pnpm concurrently 'pnpm:test-adminpage' 'pnpm:test-extensions'"
		],
		"test-php": [
			"@php tools/build-module-headings-translations.php",
			"tests/action-test-php.sh"
		],
		"watch": [
			"Composer\\Config::disableProcessTimeout",
			"pnpm run watch"
		]
	},
	"repositories": [],
	"autoload": {
		"classmap": [
			"src"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"platform": {
			"ext-intl": "0.0.0"
		},
		"autoloader-suffix": "f11009ded9fc4592b6a05b61ce272b3c_jetpackⓥ15_1_1",
		"allow-plugins": {
			"automattic/jetpack-autoloader": true,
			"automattic/jetpack-composer-plugin": true
		}
	},
	"extra": {
		"mirror-repo": "Automattic/jetpack-production",
		"release-branch-prefix": [
			"jetpack",
			"weekly",
			"monthly"
		],
		"version-constants": {
			"JETPACK__VERSION": "jetpack.php"
		},
		"wp-plugin-slug": "jetpack",
		"dev-releases": true,
		"changelogger-default-type": "other",
		"changelogger": {
			"versioning": "wordpress",
			"types": {
				"major": "Major Enhancements",
				"enhancement": "Enhancements",
				"compat": "Improved compatibility",
				"bugfix": "Bug fixes",
				"other": "Other changes <!-- Non-user-facing changes go here. This section will not be copied to readme.txt. -->"
			}
		}
	}
}
