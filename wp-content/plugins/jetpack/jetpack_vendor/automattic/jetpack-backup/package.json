{
	"private": true,
	"description": "Easily restore or download a backup of your site from a specific moment in time.",
	"homepage": "https://github.com/Automattic/jetpack/tree/HEAD/projects/packages/backup/#readme",
	"bugs": {
		"url": "https://github.com/Automattic/jetpack/labels/[Package] Backup"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Automattic/jetpack.git",
		"directory": "projects/packages/backup"
	},
	"license": "GPL-2.0-or-later",
	"author": "Automattic",
	"scripts": {
		"build": "pnpm run clean && pnpm run build-client",
		"build-client": "webpack",
		"build-concurrently": "pnpm run clean && concurrently 'pnpm:build-client' 'pnpm:build-php'",
		"build-production-concurrently": "pnpm run clean && concurrently 'NODE_ENV=production BABEL_ENV=production pnpm run build-client' && pnpm run validate",
		"clean": "rm -rf build/",
		"test": "jest --config=tests/jest.config.js",
		"test-coverage": "pnpm run test --coverage",
		"typecheck": "tsc --noEmit",
		"validate": "pnpm exec validate-es build/",
		"watch": "pnpm run build && webpack watch"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"dependencies": {
		"@automattic/jetpack-analytics": "^1.0.5",
		"@automattic/jetpack-api": "^1.0.12",
		"@automattic/jetpack-components": "^1.3.13",
		"@automattic/jetpack-connection": "^1.4.20",
		"@automattic/jetpack-shared-extension-utils": "^1.3.28",
		"@automattic/number-formatters": "^1.0.15",
		"@tanstack/react-query": "5.90.8",
		"@wordpress/api-fetch": "7.36.0",
		"@wordpress/components": "30.9.0",
		"@wordpress/data": "10.36.0",
		"@wordpress/date": "5.36.0",
		"@wordpress/element": "6.36.0",
		"@wordpress/i18n": "6.9.0",
		"moment": "2.30.1",
		"prop-types": "^15.8.1",
		"react": "18.3.1",
		"react-dom": "18.3.1"
	},
	"devDependencies": {
		"@automattic/jetpack-base-styles": "^1.0.12",
		"@automattic/jetpack-webpack-config": "workspace:*",
		"@babel/core": "7.28.4",
		"@babel/preset-env": "7.28.3",
		"@babel/runtime": "7.28.4",
		"@testing-library/dom": "10.4.1",
		"@testing-library/react": "16.3.0",
		"@testing-library/user-event": "14.6.1",
		"@types/react": "18.3.26",
		"@wordpress/browserslist-config": "6.36.0",
		"concurrently": "9.2.1",
		"jest": "30.2.0",
		"jest-environment-jsdom": "30.2.0",
		"sass-embedded": "1.87.0",
		"sass-loader": "16.0.5",
		"storybook": "10.0.8",
		"typescript": "5.9.3",
		"webpack": "5.101.3",
		"webpack-cli": "6.0.1"
	}
}
